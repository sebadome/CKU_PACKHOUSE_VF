{
  "name": "cku-packhouse-backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && powershell -NoProfile -Command \"New-Item -ItemType Directory -Force 'dist/routes' | Out-Null; Copy-Item -Force -Recurse 'src/routes/*' 'dist/routes/'\"",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "luxon": "^3.7.2",
    "mssql": "^10.0.2",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.7.1",
    "@types/mssql": "^9.1.9",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4"
  }
}
